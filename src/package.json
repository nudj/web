{
  "name": "web",
  "version": "0.0.1",
  "description": "Nudj marketing website",
  "main": "app/server",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nudj/web.git"
  },
  "author": "Nick Collings",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nudj/web/issues"
  },
  "homepage": "https://github.com/nudj/web#readme",
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "app/server/assets",
      "app/server/build*",
      "app/node_modules",
      "@nudj",
      "webpack*",
      "hot*"
    ]
  },
  "scripts": {
    "start": "yarn run build:dlls && yarn run build:client && node .",
    "dev": "yarn run build:dlls && USE_DEV_SERVER=true DEV_SERVER_PATH=https://web-wds.local.nudj.co yarn run watch:all",
    "build:client": "webpack --config ./webpack.config.client.js --bail --hide-modules",
    "build:dlls": "webpack --config ./webpack.config.dll.js --bail --hide-modules",
    "watch:all": "webpack-dev-server --config ./webpack.config.client.js & webpack --config ./webpack.config.server.js"
  },
  "dependencies": {
    "@nudj/components": "2.4.0",
    "@nudj/framework": "17.0.0",
    "@nudj/library": "6.3.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-react": "^6.23.0",
    "babel-register": "^6.26.0",
    "classnames": "^2.2.5",
    "date-fns": "^1.28.4",
    "envkey": "^1.0.0",
    "express": "^4.14.1",
    "font-awesome": "^4.7.0",
    "hamburgers": "^0.8.1",
    "hash-generator": "^0.1.0",
    "history": "^4.6.1",
    "intercom-client": "^2.8.7",
    "lodash": "^4.17.4",
    "mailgun-js": "^0.17.0",
    "passport": "^0.3.2",
    "passport-auth0": "^0.6.0",
    "prismic.io": "^3.5.6",
    "react": "^15.4.2",
    "react-clipboard.js": "^1.1.2",
    "react-collapse": "^4.0.3",
    "react-dom": "^15.4.2",
    "react-helmet": "^5.0.3",
    "react-motion": "^0.5.2",
    "react-redux": "^5.0.3",
    "react-router-dom": "^4.0.0",
    "react-router-redux": "next",
    "react-typeout": "^0.6.4",
    "redux": "^3.6.0",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.1.10",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "clean-webpack-plugin": "^0.1.19",
    "dirty-chai": "^2.0.1",
    "mocha": "^4.0.1",
    "nock": "^9.0.22",
    "nodemon": "^1.11.0",
    "proxyquire": "^1.8.0",
    "react-hot-loader": "^4.0.1",
    "sinon": "^4.0.1",
    "sinon-chai": "^2.14.0",
    "standard": "^10.0.3",
    "start-server-webpack-plugin": "^2.2.5",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1",
    "webpack-node-externals": "^1.7.2"
  }
}
